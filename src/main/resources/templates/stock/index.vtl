<h2 class="pageHeader">All Products:</h2>

<div class="container">
<table>
    <tr>
        <th>Brand:</th>
        <th>Type:</th>
        <th></th>
    </tr>
    #foreach($stock in $stockItem)
    <tr>
        <td>$stock.getBrand()</td>
        <td>$stock.getProductType()</td>
        <td><a href="/products/$stock.getId()" class="button center">Click here for details</a></td>
    </tr>
    #end
</table>
</div>



<form action="/products/new" method="get">
<input type="submit" value="Add Product" class="button center">
</form>

<form action="/Home" method="get">
<input type="submit" value="Return to homepage" class="button center">
</form>